{
  "name": "authbackend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build" : "cd bt_frontEnd && npm run build",
    "install-bt_frontEnd": "cd bt_frontEnd && npm install",
    "start": "node ./bin/www",
    "startDev": "nodemon ./bin/www",
    "bt_frontEnd": "npm start --prefix bt_frontEnd",
    "dev": "concurrently \"npm run start\" \"npm run bt_frontEnd\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix bt_frontEnd && npm run build --prefix bt_frontEnd"

  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.2",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
